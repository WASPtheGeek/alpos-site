{
  "name": "alpos-site-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "copy-prisma": "cp -r ../alpos-site-api/prisma/generated ./src/api/generated",
    "copy-models": "cp -r ../alpos-site-api/src/models ./src/api/generated",
    "copy-api-models": "rm -rf ./src/api/generated && npm run copy-prisma && npm run copy-models",
    "dev": "next dev",
    "build": "npm run copy-api-models && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@WASPtheGeek/base-components": "^0.1.19",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.79.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "axios": "^1.7.7",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
